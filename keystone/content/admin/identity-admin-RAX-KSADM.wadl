    </method>

    <method name="HEAD" id="checkTenant">
        <response status="200 203">
        </response>
        <response status="401">
        </response>
        <response status="403">
        </response>
        <response status="400">
        </response>
        <response status="404">
            <representation mediaType="application/xml" element="identity:itemNotFound"/>
